{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/api/unsplash",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1200, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/youtube",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=120, stale-while-revalidate=60"
        }
      ]
    },
    {
      "source": "/api/subscribers",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1200, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/gumroad",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=120, stale-while-revalidate=60"
        }
      ]
    },
    {
      "source": "/api/github",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=120, stale-while-revalidate=60"
        }
      ]
    }
  ]
}
